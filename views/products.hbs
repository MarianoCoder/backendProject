<h1>Products</h1>

<table id="main">
    <tr>
        <th>Nombre</th>
        <th>Descripción</th>
        <th>Código</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Categoría</th>
    </tr>

    {{#each products}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.code}}</td>
        <td>{{this.price}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.category}}</td>
    </tr>
    {{/each}}

</table>